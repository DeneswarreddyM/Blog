<div class="author">
    <div class="author__heading">Search Results</div>
</div>

<section class="article">
    <ul class="article-ul">
        <% data.forEach(post=> { %>
          <li>
            <a href="/post/<%=post._id%>">
              <span><%= post._id %></span>
              <span><%=post.title%> </span>
              <span class="article-list_date"><%= post.createdAt.toDateString() %></span> 
            </a>
          </li>
          <% }) %>
      </ul>
</section>